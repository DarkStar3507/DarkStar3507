#include <iostream>
#include <vector>

class Lista {
public:
    std::vector<int> elementos;

public:
    bool esta_vacia() {
        return elementos.empty();
    }
};

int main() {
    Lista lista;
    if (lista.esta_vacia()) {
        std::cout << "La lista esta vacia" << std::endl;
    } else {
        std::cout << "La lista no esta vacia" << std::endl;
    }
    lista.elementos.push_back(1);
    if (lista.esta_vacia()) {
        std::cout << "La lista esta vacia" << std::endl;
    } else {
        std::cout << "La lista no esta vacia" << std::endl;
    }
    return 0;
}
